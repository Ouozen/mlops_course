    "version": "0.2.0",
    "configurations": [
        {
            "name": "infer file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--weight_file",
                "model.json",
                "--download_files",
                "False"
            ]
        },
        {
            "name": "train file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--download_files",
                "True",
                "--weight_file",
                "model.json"
            ]
        }
    ]
}
